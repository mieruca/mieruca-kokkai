{
  "name": "mieruca-kokkai",
  "version": "1.0.0",
  "description": "Japanese Diet members information scraper using Playwright",
  "main": "index.js",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "playwright test",
    "prepare": "husky",
    "lint": "biome lint src/ tests/",
    "format": "biome format src/ tests/ --write",
    "check": "biome check src/ tests/",
    "typecheck": "tsc --noEmit -p tsconfig.json && tsc --noEmit -p tsconfig.tests.json"
  },
  "keywords": [
    "playwright",
    "scraping",
    "japanese-diet",
    "politics"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "^2.1.3",
    "@playwright/test": "^1.40.0",
    "@tsconfig/strictest": "^2.0.5",
    "@types/node": "^20.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.4",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "playwright": "^1.40.0"
  },
  "lint-staged": {
    "{src,tests}/**/*.{ts,js}": [
      "biome check --write",
      "biome format --write"
    ]
  }
}
